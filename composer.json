{
    "name": "hypervel/scheduling",
    "type": "library",
    "description": "The scheduling package for Hypervel.",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf",
        "crontab",
        "scheduling",
        "swoole",
        "hypervel"
    ],
    "autoload": {
        "psr-4": {
            "Hypervel\\Scheduling\\": "src/"
        }
    },
    "require": {
        "php": "^8.2",
        "ext-mbstring": "*",
        "hypervel/support": "^0.1",
        "dragonmantank/cron-expression": "^3.3.2",
        "symfony/console": "^5.4|^6.4|^7.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.5"
    },
    "suggest": {
        "hypervel/prompt": "Required to run schedule:test command.",
        "friendsofhyperf/pretty-console": "Required to run schedule:run command. (~3.1.0)"
    },
    "extra": {
        "hyperf": {
            "config": "Hypervel\\Scheduling\\ConfigProvider"
        },
        "branch-alias": {
            "dev-main": "0.1-dev"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}